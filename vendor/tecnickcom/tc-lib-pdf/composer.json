        "tecnickcom/tc-lib-unicode-data": "^1.6.2",
        "tecnickcom/tc-lib-unicode": "^1.3.2",
        "tecnickcom/tc-lib-pdf-page": "^2.1.2",
        "tecnickcom/tc-lib-pdf-graph": "^1.1.2"
    },
    "require-dev": {
        "apigen/apigen" : "^4.1.2",
        "bartlett/php-compatinfo" : "^5.0.0",
        "pdepend/pdepend" : "^2.2.2",
        "phploc/phploc" : "^2.1.5|^3.0.0",
        "phpmd/phpmd" : "^2.3.2",
        "phpunit/phpunit": "4.8.19|^5.0.0",
        "sebastian/phpcpd" : "^2.0.2",
        "squizlabs/php_codesniffer": "^2.5.0|^3.0.0"
    },
    "autoload": {
        "psr-4":{
            "Com\\Tecnick\\Pdf\\": "src"
