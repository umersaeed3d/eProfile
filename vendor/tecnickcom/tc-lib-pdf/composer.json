    "require": {
        "php": ">=5.4",
        "tecnickcom/tc-lib-barcode": "^1.10.3",
        "tecnickcom/tc-lib-color": "^1.11.2",
        "tecnickcom/tc-lib-pdf-image": "^1.1.2",
        "tecnickcom/tc-lib-pdf-font": "^1.7.2",
